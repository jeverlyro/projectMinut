{
  "expo": {
    "name": "Minut App",
    "slug": "minut",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Minut App to use your location to show your position on the map."
        }
      ]
    ],
    "assetBundlePatterns": ["**/*"],
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "width": 512,
      "height": 512,
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyBsUtVXIMqoL4vhIV72ZHYs-8eg3wdRV-c"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBsUtVXIMqoL4vhIV72ZHYs-8eg3wdRV-c"
        }
      },
      "package": "com.rechtownsyou.minut"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "207dd93a-20be-4b28-a87d-3fe2e78087de"
      }
    }
  }
}
